

<div id="login-page">
<h1>Log in</h1>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field login-email">
    <%= f.email_field :email, :placeholder => "Email", autofocus: true %>
  </div>

  <div class="field login-password">
    <%= f.password_field :password, :placeholder => "Password", autocomplete: "off" %>
  </div>



  <div class="actions">
    <%= f.submit "Log in", id: "user_login" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>

</div>





