<!-- <form class="form-signin"> -->
<div id="login-page">
<h2 class="form-signin-heading">Please sign in</h1>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <label for="inputEmail" class="sr-only">Email address</label>
  <div class="field login-email">
    <%= f.email_field :email, :placeholder => "Email", autofocus: true,:class => "form-control" %>
  </div>
  <div class="field login-password">
    <%= f.password_field :password, :placeholder => "Password", autocomplete: "off", :class => "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit "Log in", id: "user_login", :class =>"log-in-button" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</div>
