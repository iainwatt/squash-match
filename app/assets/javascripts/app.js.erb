// #= depend_on_asset "index.html"

var app = angular.module('SquinderApp', ['ngRoute', 'ngAnimate', 'ngMap']);

app.value( 'gps', { 
  userLat: '51.520151399999996',
  userLon: '-0.0704819'
});



app.config(['$routeProvider', '$locationProvider', function($routeProvider, $locationProvider) {
  $routeProvider
  .when('/home', {
    controller: 'CentreController',
    templateUrl: "<%= asset_path('index.html') %>"
  })
  .when('/profile', {
    controller: 'ProfileController',
    templateUrl: "<%= asset_path('profile.html') %>"   
    })
  .otherwise({
    redirectTo: '/home'
  });

  // $locationProvider.html5Mode(true);

}]);